{
"manifest_version": 2,
    "name": "Mitch",
    "description": "Mitch. An extension that makes you snort-laugh with a Mitch Hedberg joke with every new tab.",
    "version": "1.0",
    "chrome_url_overrides" : {
        "newtab": "mitch.html"
    },
    "browser_action": {
        "default_title": "Mitch",
        "default_icon": "icon_16.png"
    },
    "permissions": [
      "tabs",
      "chrome://favicon/",
      "storage"
    ],
    "icons": {
      "16": "icon_16.png",
      "48": "icon_48.png",
      "128": "icon_128.png"
        },
    "background": {
      "scripts": ["jquery-3.2.1.min.js", "background.js"],
      "persistent": false
  },
    "content_scripts": [{
      "matches": ["http://*/*"],
      "js": ["jquery-3.2.1.min.js", "content_mitch.js"]
      }]
}
